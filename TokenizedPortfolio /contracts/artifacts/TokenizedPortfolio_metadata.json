		"devdoc": {
			"details": "A tokenized portfolio for managing multiple assets, transferring, rebalancing, checking risk, staking, flash loans, governance tokens, dynamic fees, insurance, referral system, and governance voting.",
			"errors": {
				"AddressEmptyCode(address)": [
					{
						"details": "There's no code at `target` (it is not a contract)."
					}
				],
				"AddressInsufficientBalance(address)": [
					{
						"details": "The ETH balance of the account is not enough to perform the operation."
					}
				],
				"FailedInnerCall()": [
					{
						"details": "A call to an address target failed. The target may have reverted."
					}
				],
				"SafeERC20FailedOperation(address)": [
					{
						"details": "An operation with an ERC20 token failed."
					}
				]
			},
			"kind": "dev",
			"methods": {
				"addAsset(string,uint256,uint256)": {
					"details": "Adds an asset to the portfolio."
				},
				"applyDynamicFees(uint256)": {
					"details": "Applies dynamic management and performance fees."
				},
				"buyInsurance(uint256,uint256)": {
					"details": "Allows users to buy an insurance policy for their portfolio."
				},
				"checkRisk()": {
					"details": "Checks if the portfolio violates predefined risk thresholds."
				},
				"claimInsurance()": {
					"details": "Claims insurance for the portfolio."
				},
				"claimStakingRewards()": {
					"details": "Reward long-term stakers based on the staking duration."
				},
				"createProposal(string,uint256)": {
					"details": "Create a new governance proposal with a time-locked voting deadline."
				},
				"emergencyWithdrawAllAssets(address[])": {
					"details": "Emergency withdrawal of all assets by the portfolio owner."
				},
				"getLatestPrice(string)": {
					"details": "Gets the latest price of an asset from a Chainlink oracle."
				},
				"initializePortfolio()": {
